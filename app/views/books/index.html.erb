<h1>Books</h1>

<% @books.each do |books| %>


  <p>Title</p>
  <p><%= books.title %></p>

  <p>Body</p>
  <p><%= books.body %></p>
<% end %>
<h2>New book</h2>
<%= form_with model: @book do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>

  <h4>Body</h4>
  <%= f.text_area :body %>

  <%= f.submit 'Create Book' %>

<% end %>


